   pod 'FacebookLogin'
   pod 'Bolts'
  # Pods for Monitor

  target 'MonitorTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MonitorUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
